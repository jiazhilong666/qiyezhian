<script type="text/javascript">

var redirect = false;
var server_url = document.URL.split("/");
for (var i = 0; i < server_url.length; i++) {
    if (server_url[i] == "") {         
      server_url.splice(i, 1);
      i--;
    }
}
var new_url ="https://";
if(server_url.length > 5){
    for (var i in server_url){
        if(i != 0 && i < server_url.length - 1 ){
            new_url += server_url[i];
            if(i < server_url.length - 2){
            	new_url += "/";
            }
        } 
    }
    //console.log(new_url);
    if(!new_url.match(/\/erreur/g)) {
    	//console.log('redirect');
		window.location.href = new_url;
		redirect = true;
	}
}
delete server_url;
delete new_url;
$(document).ready(function() {
	setTimeout(function() {
		if(!redirect) {
	  		window.location.href = 'https://www.dior.cn/home/zh_cn';
	  	}
	}, 5000);
});

